<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_photo"
    app:startDestination="@id/galleryFragment">

    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.yuan.testjetpack.fragment.GalleryFragment"
        android:label="Gallery"
        tools:layout="@layout/gallery_fragment">
        <action
            android:id="@+id/action_galleryFragment_to_pagerPhotoFragment"
            app:destination="@id/pagerPhotoFragment" />
    </fragment>
    <fragment
        android:id="@+id/pagerPhotoFragment"
        android:name="com.yuan.testjetpack.fragment.PagerPhotoFragment"
        android:label="pagerPhoto"
        tools:layout="@layout/pager_photo_fragment" />
</navigation>